{
  "name": "docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "node generatePluginsPages.js latest && docusaurus start",
    "start-debug": "cd ../js-api && npm install && cd ../docusaurus && node generatePluginsPages.js create && TYPEDOC_WATCH=true docusaurus start --config docusaurus-debug.config.js",
    "build": "node generatePluginsPages.js && docusaurus build",
    "generate-plugins-pages": "node generatePluginsPages.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "markdown-to-jsx": "^7.1.9",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@droppedcode/typedoc-plugin-copy-assets": "^1.0.6",
    "@tsconfig/docusaurus": "^1.0.6",
    "docusaurus-plugin-typedoc": "^0.18.0",
    "docusaurus-theme-search-typesense": "^0.8.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-markdown": ">=3.14.0 <3.15.0",
    "typedoc-plugin-replace-text": "2.0.0",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
