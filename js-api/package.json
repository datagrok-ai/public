{
  "name": "datagrok-api",
  "version": "1.26.8",
  "description": "",
  "main": "./datagrok.js",
  "types": "./datagrok.d.ts",
  "typesVersions": {
    "*": {
      "grok": ["./grok.d.ts"],
      "grok.js": ["./grok.d.ts"],
      "ui": ["./ui.d.ts"],
      "ui.js": ["./ui.d.ts"],
      "dg": ["./dg.d.ts"],
      "dg.js": ["./dg.d.ts"],
      "node-api": ["./node-api.d.ts"]
    }
  },
  "exports": {
    "./grok": {
      "types": "./grok.d.ts",
      "default": "./grok.js"
    },
    "./grok.js": {
      "types": "./grok.d.ts",
      "default": "./grok.js"
    },
    "./ui": {
      "types": "./ui.d.ts",
      "default": "./ui.js"
    },
    "./ui.js": {
      "types": "./ui.d.ts",
      "default": "./ui.js"
    },
    "./dg": {
      "types": "./dg.d.ts",
      "default": "./dg.js"
    },
    "./dg.js": {
      "types": "./dg.d.ts",
      "default": "./dg.js"
    },
    "./node-api": {
      "types": "./node-api.d.ts",
      "default": "./node-api.js"
    },
    "./src/const": {
      "types": "./src/const.d.ts",
      "default": "./src/const.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@datagrok-libraries/chem-meta": "^1.0.12",
    "@types/react": "^18.3.11",
    "@types/wu": "^2.1.44",
    "cash-dom": "^8.1.5",
    "dayjs": "^1.11.10",
    "openchemlib": "^7.2.3",
    "react": "^18.3.1",
    "rxjs": "^6.5.5",
    "typeahead-standalone": "4.14.1",
    "wu": "^2.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^5.0.0",
    "null-loader": "^4.0.1",
    "eslint": "^7.23.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^2.0.0",
    "svg-loader": "^0.0.2",
    "ts-loader": "^8.1.0",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^4.2.3",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.6.0",
    "utf-8-validate": "^6.0.5",
    "bufferutil": "^4.0.9"
  },
  "scripts": {
    "link": "npm link",
    "link-all": "npm link @datagrok-libraries/chem-meta",
    "build-ts": "tsc && eslint ./src/*.js ./dg.js ./ui.js ./grok.js --fix",
    "build-js-api": "git clean -f -X -d ./src && tsc && webpack",
    "build": "tsc && webpack",
    "build-docker": "tsc && webpack --config webpack.config.docker.js",
    "build-all": "npm --prefix ./../libraries/chem-meta run build && npm run build"
  }
}
