    "dayjs": "^1.10.6",
    "file-loader": "^6.2.0",
    "parquet-wasm": "0.6.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
