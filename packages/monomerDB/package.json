{
  "name": "@datagrok/monomerdb",
  "friendlyName": "monomerDB",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Davit Rizhinashvili",
    "email": "drizhinashvili@datagrok.ai"
  },
  "dependencies": {
    "@datagrok-libraries/bio": "^5.60.0",
    "@datagrok-libraries/db-explorer": "^1.3.0",
    "@datagrok-libraries/utils": "^4.6.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cash-dom": "^8.1.5",
    "css-loader": "^7.1.2",
    "datagrok-api": "^1.26.0",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0"
  },
  "devDependencies": {
    "@types/wu": "^2.1.44",
    "datagrok-tools": "^5.0.0",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "link-all": "grok link",
    "debug-monomerdb": "webpack && grok publish",
    "release-monomerdb": "webpack && grok publish --release",
    "build-monomerdb": "webpack",
    "build": "grok api && grok check && webpack",
    "debug-monomerdb-dev": "webpack && grok publish dev",
    "release-monomerdb-dev": "webpack && grok publish dev --release",
    "debug-monomerdb-local": "webpack && grok publish local",
    "release-monomerdb-local": "webpack && grok publish local --release",
    "debug-monomerdb-localhost": "webpack && grok publish localhost",
    "release-monomerdb-localhost": "webpack && grok publish localhost --release",
    "debug-monomerdb-release": "webpack && grok publish release",
    "release-monomerdb-release": "webpack && grok publish release --release",
    "debug-monomerdb-test": "webpack && grok publish test",
    "release-monomerdb-test": "webpack && grok publish test --release"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/monomerDB"
  },
  "meta": {
    "semanticTypes": [
      {
        "semType": "DG_MONOMERDB_ID",
        "description": "Monomer Identifier",
        "parsers": [
          {
            "regexp": "GROKMONO-\\d"
          }
        ]
      }
    ]
  },
  "overrides": {
    "datagrok-api": "$datagrok-api"
  }
}
