  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "@types/jest": "^27.0.0",
    "jest": "^27.0.0",
    "jest-html-reporter": "^3.5.0",
    "path": "^0.12.7",
    "puppeteer": "^13.7.0",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
