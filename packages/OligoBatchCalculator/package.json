    "cash-dom": "^8.1.5",
    "datagrok-api": "^1.25.0",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "path": "^0.12.7",
    "ts-loader": "^9.2.6",
    "typescript": "^5.0.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
