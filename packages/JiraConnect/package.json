{
  "name": "@datagrok/jiraconnect",
  "friendlyName": "Jira Connect",
  "version": "1.2.3",
  "description": "Pakcages for Jira integration into Datagrok",
  "author": {
    "name": "Savenko Stanislav",
    "email": "ssavenko@datagrok.ai"
  },
  "dependencies": {
    "@datagrok-libraries/utils": "^4.6.5",
    "cash-dom": "^8.1.5",
    "cors": "^2.8.5",
    "datagrok-api": "^1.26.0",
    "dayjs": "^1.11.13",
    "@datagrok-libraries/test": "^1.1.0"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "datagrok-tools": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "link-all": "grok link",
    "debug-jiraconnect": "webpack && grok publish",
    "release-jiraconnect": "webpack && grok publish --release",
    "build-jiraconnect": "webpack",
    "build": "grok api && grok check --soft && grok api && webpack",
    "debug-jiraconnect-public": "webpack && grok publish public",
    "release-jiraconnect-public": "webpack && grok publish public --release",
    "debug-jiraconnect-local": "webpack && grok publish local",
    "release-jiraconnect-local": "webpack && grok publish local --release",
    "debug-jiraconnect-release": "webpack && grok publish release",
    "release-jiraconnect-release": "webpack && grok publish release --release",
    "debug-jiraconnect-release-ec2.datagrok.ai": "webpack && grok publish release-ec2.datagrok.ai",
    "release-jiraconnect-release-ec2.datagrok.ai": "webpack && grok publish release-ec2.datagrok.ai --release",
    "debug-jiraconnect-core": "webpack && grok publish core",
    "release-jiraconnect-core": "webpack && grok publish core --release",
    "debug-jiraconnect-ec2": "webpack && grok publish ec2",
    "release-jiraconnect-ec2": "webpack && grok publish ec2 --release",
    "debug-jiraconnect-test": "webpack && grok publish test",
    "release-jiraconnect-test": "webpack && grok publish test --release"
  },
  "canEdit": [
    "Administrators"
  ],
  "canView": [
    "All users"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/JiraConnect"
  },
  "overrides": {
    "datagrok-api": "$datagrok-api"
  }
}
