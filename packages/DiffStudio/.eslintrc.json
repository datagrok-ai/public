{
    "env": {
      "browser": true,
      "es2022": true
    },
    "extends": [
      "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
          "@typescript-eslint/no-unused-vars": ["warn", { "varsIgnorePattern": "^(_|ui$|grok$|DG$)", "argsIgnorePattern": "^_"}],
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "error",
        120
      ],
      "require-jsdoc": "off",
      "valid-jsdoc": "off",
      "spaced-comment": "off",
      "linebreak-style": "off",
      "curly": [
        "error",
        "multi-or-nest"
      ],
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "block-spacing": 2
    }
  }