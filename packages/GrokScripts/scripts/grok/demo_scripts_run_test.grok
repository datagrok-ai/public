#tags: test

rScripts:ACF("TSLA", ["Date", "Open", "High", "Low", "Close", "Volume", "Adjusted"])
rScripts:ArimaForecasting("TSLA", "Date", "Close", 4, 3, 3, 25)
rScripts:ANOVA("beer", "BJCP Categories", "IBU")
