rScripts:MANOVA("beer", "IBU", "SRM", "OG")
rScripts:PCA("cars", ["diesel", "turbo", "two.doors", "hatchback", "wheel.base", "length", "width", "height", "curb.weight", "eng.size", "horsepower", "peak.rpm", "symbol", "city.mpg", "highway.mpg", "price"], 2, true, true)
rScripts:PLS("cars", "price", ["diesel", "turbo", "two.doors", "hatchback", "wheel.base", "length", "width", "height", "curb.weight", "eng.size", "horsepower", "peak.rpm", "symbol", "city.mpg", "highway.mpg"], 2)
