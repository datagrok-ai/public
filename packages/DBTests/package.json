    "debug-dbtests-localhost": "webpack && grok publish localhost",
    "release-dbtests-localhost": "webpack && grok publish localhost --release",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "test": "grok test"
