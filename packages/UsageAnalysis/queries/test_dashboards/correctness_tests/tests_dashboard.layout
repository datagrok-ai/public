      "stable": {
        "formula": "And(${1}== \"passed\",And(${2}== \"passed\",And(${3}== \"passed\",And(${4}== \"passed\",${5}== \"passed\"))))"
      },
      "failing": {
        "formula": "if (IsEmpty(${1}), Or(${1}== \"failed\",Or(${2}== \"failed\",Or(${3}== \"failed\",Or(${4}== \"failed\",${5}== \"failed\")))), ${1} == \"failed\")"
      },
      "flaking": {
        "formula": "Or(${1 first(flaking)}== true,Or(${2 first(flaking)}== true,Or(${3 first(flaking)}== true,Or(${4 first(flaking)}== true,${5 first(flaking)}== true))))"
      },
      "needs_attention": {
        "formula": "And(${failing}, Not(${ignore?}))"
      },
      "1 tickets status": {
        "formula": "UsageAnalysis:getTicketsVerdict(${1 result})",
        ".%initialName": "1 concat unique(result) ticket verdict",
        ".%formula-column-is-vector-func": "true"
      },
      "ignore status": {
        "formula": "UsageAnalysis:getTicketsVerdict(${ignoreReason})",
        ".%initialName": "ignoreReason ticket verdict",
        ".%formula-column-is-vector-func": "true"
