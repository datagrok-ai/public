{
  "name": "@datagrok/biologics",
  "friendlyName": "Biologics",
  "version": "1.0.5",
  "description": "Demo Database wrapper for ADCs",
  "author": {
    "name": "Davit Rizhinashvili",
    "email": "drizhinashvili@datagrok.ai"
  },
  "dependencies": {
    "@datagrok-libraries/db-explorer": "^1.3.0",
    "@datagrok-libraries/utils": "^4.6.9",
    "cash-dom": "^8.1.5",
    "datagrok-api": "^1.26.0",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "datagrok-tools": "^4.14.65",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "link-all": "grok link",
    "debug-biologics": "webpack && grok publish",
    "release-biologics": "webpack && grok publish --release",
    "build-biologics": "webpack",
    "build": "grok api && grok check && webpack",
    "debug-biologics-dev": "webpack && grok publish dev",
    "release-biologics-dev": "webpack && grok publish dev --release",
    "debug-biologics-local": "webpack && grok publish local",
    "release-biologics-local": "webpack && grok publish local --release",
    "debug-biologics-localhost": "webpack && grok publish localhost",
    "release-biologics-localhost": "webpack && grok publish localhost --release",
    "debug-biologics-release": "webpack && grok publish release",
    "release-biologics-release": "webpack && grok publish release --release",
    "debug-biologics-test": "webpack && grok publish test",
    "release-biologics-test": "webpack && grok publish test --release"
  },
  "canEdit": [
    "Administrators"
  ],
  "canView": [
    "All users"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/Biologics"
  },
  "meta": {
    "semanticTypes": [
      {
        "semType": "DG_BIOLOGICS_DRUG_ID",
        "description": "Drug identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKMOL-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_PEPTIDE_ID",
        "description": "Peptide identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKPEP-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_SEQUENCE_ID",
        "description": "Sequence identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKSEQ-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_LINKER_ID",
        "description": "Linker identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKLINKER-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_ADC_ID",
        "description": "ADC identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKADC-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_ORGANISM_ID",
        "description": "ORGANISM identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKORG-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_PURIFICATION_ID",
        "description": "Sequence identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKPUR-\\d{6}"
          }
        ]
      },
      {
        "semType": "DG_BIOLOGICS_EXPRESSION_ID",
        "description": "Sequence identifier from the Biologics package",
        "parsers": [
          {
            "regexp": "GROKEXP-\\d{6}"
          }
        ]
      }
    ]
  }
}
