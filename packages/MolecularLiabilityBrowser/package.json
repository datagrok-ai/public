{
  "name": "@datagrok/molecular-liability-browser",
  "fullName": "MolecularLiabilityBrowser",
  "version": "1.3.1",
  "author": {
    "name": "Aleksandr Tanas",
    "email": "atanas@datagrok.ai"
  },
  "description": "MolecularLiabilityBrowser is a [package](https://datagrok.ai/help/develop/develop#packages) for the [Datagrok](https://datagrok.ai) platform to analyse variable region sequences of antibodies.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datagrok-ai/public.git",
    "directory": "packages/MolecularLiabilityBrowser"
  },
  "properties": [
    {
      "name": "IndexedDB",
      "propertyType": "bool",
      "defaultValue": false,
      "nullable": false
    },
    {
      "name": "DataSource",
      "propertyType": "string",
      "choices": [
        "Files",
        "Database",
        "Test"
      ],
      "defaultValue": "Test",
      "nullable": false
    }
  ],
  "sources": [
    "css/ngl.css",
    "css/pviz-core.css",
    "css/additional-styles.css",
    "vendor/pviz-bundle.min.js",
    "common/ngl_viewer/ngl.js",
    "css/vd-regions-viewer.css"
  ],
  "dependencies": {
    "@datagrok-libraries/bio": "^5.13.0",
    "@datagrok-libraries/utils": "^1.15.5",
    "@deck.gl/core": "8.8.12",
    "cash-dom": "8.1.1",
    "datagrok-api": "^1.8.2",
    "dexie": "^3.2.2",
    "logojs-react": "^2.1.1",
    "mjolnir.js": "^2.7.0",
    "rxjs": "^6.5.5",
    "ts-transformer-keys": "^0.4.3",
    "uint8-to-base64": "^0.2.0",
    "weighted": "^1.0.0",
    "wu": "latest"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.24",
    "@types/node-fetch": "^2.6.2",
    "@types/wu": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-google": "latest",
    "jest": "^27.5.1",
    "jest-html-reporter": "^3.6.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "puppeteer": "^13.7.0",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "latest"
  },
  "scripts": {
    "link-api": "npm link datagrok-api",
    "link-utils": "npm link @datagrok-libraries/utils",
    "link-bio": "npm link @datagrok-libraries/bio",
    "link-all": "npm link datagrok-api @datagrok-libraries/utils @datagrok-libraries/bio",
    "debug-local": "grok publish local",
    "release-local": "grok publish local --release",
    "build": "webpack",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint \"./src/**/*.ts\" --fix",
    "test": "jest",
    "test-dev": "set HOST=dev && jest",
    "test-local": "set HOST=localhost && jest"
  },
  "grokDependencies": {
    "@datagrok/bio": "2.1.7",
    "@datagrok/phylo-tree-viewer": "1.2.0",
    "@datagrok/molecular-liability-browser-data": "^2.0.0"
  },
  "canEdit": [
    "Developers"
  ],
  "category": "Bioinformatics"
}
